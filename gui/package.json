{
    "name": "zdpack-gui",
    "version": "1.0.4",
    "description": "GUI for zdpack",
    "scripts": {
        "copyzdp": "copyfiles --up 3 \"../cli/dist/*.*\" \"src/packer\"",
        "start": "npm run copyzdp && electron .",
        "build": "npm run copyzdp && electron-builder build"
    },
    "build": {
        "appId": "zdpack-gui",
        "win": {
            "target": "portable",
            "icon": "build/icon.ico"
        }
    },
    "main": "src/index.js",
    "author": "mathgeniuszach",
    "license": "ISC",
    "devDependencies": {
        "copyfiles": "^2.4.1",
        "electron": "^13.1.4",
        "electron-builder": "^22.11.7",
        "eslint": "^7.29.0"
    },
    "dependencies": {
        "@electron/remote": "^1.2.0",
        "cli-progress": "^3.9.0",
        "commander": "^8.0.0",
        "fs-extra": "^10.0.0",
        "hjson": "^3.2.2",
        "js-yaml": "^4.1.0",
        "jszip": "^3.6.0",
        "lodash": "^4.17.21",
        "nid": "^1.3.3",
        "pegjs": "^0.10.0",
        "platform-folders": "^0.5.4",
        "react": "^16.14.0",
        "zip-dir": "^2.0.0"
    }
}
